<beans xmlns = "http://www.springframework.org/schema/beans"
   xmlns:xsi = "http://www.w3.org/2001/XMLSchema-instance"
   xmlns:context = "http://www.springframework.org/schema/context"
   xsi:schemaLocation = "http://www.springframework.org/schema/beans
   http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
   http://www.springframework.org/schema/context
   http://www.springframework.org/schema/context/spring-context-3.0.xsd">
   
   <context:annotation-config></context:annotation-config>
   <bean id="pump" class="com.hari.AutoWire_Xml_01.Pump"></bean>
   
   <bean id="manLungs" class="com.hari.AutoWire_Xml_01.Lungs">
     <property name="noOfLungs" value="2"></property>
     <property name="animal" value="Men"></property>
   </bean>
   
   <bean id="catLungs" class="com.hari.AutoWire_Xml_01.Lungs">
     <property name="noOfLungs" value="5"></property>
     <property name="animal" value="Cat"></property>
   </bean>
    
   <bean id="heart" class="com.hari.AutoWire_Xml_01.Heart"></bean>
   
   
   </beans>