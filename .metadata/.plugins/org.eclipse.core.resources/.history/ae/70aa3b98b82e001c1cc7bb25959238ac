package com.hari.PropFile;

public class Heart {
	
	int noOfHeart;
	String animal;
	
	
	public void setNoOfHeart(int noOfHeart) {
		this.noOfHeart = noOfHeart;
	}
	public void setAnimal(String animal) {
		this.animal = animal;
	}
	public int getNoOfHeart() {
		return noOfHeart;
	}
	public String getAnimal() {
		return animal;
	}
}
